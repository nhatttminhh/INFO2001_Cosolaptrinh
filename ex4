#include <stdio.h>

int main() {
    int a, b;

    printf("Nhập số nguyên a: ");
    scanf("%d", &a);

    printf("Nhập số nguyên b: ");
    scanf("%d", &b);

    if (a > b) {
        printf("Giá trị lớn nhất là: %d\n", a);
    } else {
        printf("Giá trị lớn nhất là: %d\n", b);
    }

    return 0;
}
